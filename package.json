{
    "name": "forcecode-ui",
    "description": "The UI Pieces of ForceCode",
    "version": "0.0.11",
    "author": "Brett Wagner",
    "bugs": "https://github.com/daishi4u/forcecode-ui/issues",
    "main": "dist/",
    "dependencies": {
        "compression": "^1.7.4",
        "express": "^4.17.1",
        "helmet": "^4.6.0"
    },
    "devDependencies": {
        "eslint": "^7.25.0",
        "lwc-services": "^3.0.4",
        "prettier": "^2.2.1"
    },
    "engines": {
        "node": ">=10.13.0",
        "npm": ">=6.4.1",
        "yarn": ">=1.9.4"
    },
    "homepage": "https://github.com/daishi4u/forcecode-ui",
    "keywords": [
        "lwc"
    ],
    "license": "MIT",
    "repository": "https://github.com/daishi4u/forcecode-ui",
    "scripts": {
        "build": "lwc-services build -m production",
        "build:development": "lwc-services build",
        "lint": "eslint ./src/**/*.js",
        "prettier": "prettier --write \"**/*.{css,html,js,json,md,ts,yaml,yml}\"",
        "prettier:verify": "prettier --list-different \"**/*.{css,html,js,json,md,ts,yaml,yml}\"",
        "serve": "node scripts/server.js",
        "test:unit": "lwc-services test:unit",
        "test:unit:coverage": "lwc-services test:unit --coverage",
        "test:unit:debug": "lwc-services test:unit --debug",
        "test:unit:watch": "lwc-services test:unit --watch",
        "watch": "lwc-services watch"
    }
}
